{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": [ "../../src/", "../../jre/src"],
        "exclude": [ "../../lib/" ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "./site/plugins/docs/public/docs/templates/jsdoc3Template",
        "encoding": "utf8",
        "destination": "./site/plugins/docs/public/docs/jsdoc",
        "recurse": true
    }
}